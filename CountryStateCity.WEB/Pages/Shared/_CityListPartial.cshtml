@using CountryStateCity.WEB.ViewModels;
@model List<CityViewModel>
@{

}


<h2>City</h2>
<input class="form-control" id="cityInput" type="text" placeholder="Search..">
@if (Model.Count > 0)
{
<br>
    <ul class="list-group list-unstyled list-container" id="cityList">
    @foreach (var city in Model)
    {
         <li class="list-group-item">
                @city.Name
        </li>
    }
</ul>
}
else{
    <p class="mt-3">No City Found</p>
}